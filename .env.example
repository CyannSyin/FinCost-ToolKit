# API Keys - Loaded from environment variables only
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key (required if using OpenAI provider)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Aihubmix API Key (required if using aihubmix provider)
AIHUBMIX_API_KEY=your-aihubmix-api-key-here

# Aihubmix Base URL (optional, can also be set in config.json)
# If not set here, must be provided in config.json
AIHUBMIX_BASE_URL=https://api.example.com/v1
